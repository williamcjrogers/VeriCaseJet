{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3BucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:GetObjectVersion",
                "s3:PutObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::vericase-docs-prod-526015377510",
                "arn:aws:s3:::vericase-docs-prod-526015377510/*"
            ]
        },
        {
            "Sid": "S3TransferAcceleration",
            "Effect": "Allow",
            "Action": [
                "s3:GetAccelerateConfiguration"
            ],
            "Resource": "arn:aws:s3:::vericase-docs-prod-526015377510"
        }
    ]
}
