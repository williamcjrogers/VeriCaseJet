{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
        "bootstrap": "readonly",
        "agGrid": "readonly",
        "Chart": "readonly",
        "d3": "readonly",
        "pdfjsLib": "readonly",
        "showToast": "readonly",
        "showError": "readonly",
        "API_BASE_URL": "readonly",
        "AUTH_TOKEN_KEY": "readonly",
        "secureApiFetch": "readonly",
        "VeriCaseConfig": "readonly",
        "escapeHtml": "readonly",
        "getCsrfToken": "readonly",
        "getApiUrl": "readonly",
        "addStakeholderRow": "readonly",
        "addKeywordRow": "readonly",
        "addLegalTeamRow": "readonly",
        "addHeadOfClaimRow": "readonly",
        "addCaseKeywordRow": "readonly",
        "addDeadlineRow": "readonly",
        "handleIntelligentQuickAction": "readonly",
        "module": "readonly"
    },
    "rules": {
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
        "no-undef": "warn",
        "no-console": "off"
    }
}
